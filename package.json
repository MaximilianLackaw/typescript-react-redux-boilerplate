{
  "name": "typescript-react-redux-boilerplate",
  "version": "1.0.0",
  "private": true,
  "description": "Base project for setting up a frontend application using TypeScript and React",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "test": "jest --config jest.config.js --watchAll",
    "test:coverage": "jest --config jest.config.js --coverage",
    "test:ci": "jest --config jest.config.js --ci  --coverage",
    "lint": "npm-run-all lint:ts lint:css",
    "lint:ts": "tslint \"./src/**/*.ts?(x)\" --format stylish",
    "lint:css": "stylelint \"src/**/*.css\"",
    "start": "webpack-dev-server --config ./webpack.config.js --mode development --open",
    "prebuild": "npm-run-all clean lint test:ci",
    "build": "webpack -p --progress --colors"
  },
  "keywords": [
    "boilerplate",
    "typescript",
    "react",
    "redux",
    "react-router",
    "jest",
    "postcss",
    "webpack"
  ],
  "devDependencies": {
    "@types/classnames": "^2.2.6",
    "@types/enzyme": "^3.1.13",
    "@types/history": "^4.7.0",
    "@types/jest": "^23.3.1",
    "@types/node": "^10.5.8",
    "@types/react": "^16.4.9",
    "@types/react-dom": "^16.0.7",
    "@types/react-redux": "^6.0.6",
    "@types/react-router": "^4.0.30",
    "@types/react-router-dom": "^4.3.0",
    "@types/redux-actions": "^2.3.0",
    "@types/webpack": "^4.4.9",
    "@types/webpack-env": "1.13.6",
    "awesome-typescript-loader": "^5.2.0",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^1.0.0",
    "enzyme": "^3.4.1",
    "enzyme-adapter-react-16": "^1.2.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "history": "^4.7.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.5.0",
    "mini-css-extract-plugin": "^0.4.1",
    "npm-run-all": "^4.1.3",
    "postcss": "^7.0.2",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-css-reset": "^1.0.2",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^12.0.0",
    "postcss-loader": "^3.0.0",
    "postcss-reporter": "^5.0.0",
    "postcss-url": "^8.0.0",
    "prettier": "^1.14.2",
    "react-hot-loader": "^4.3.5",
    "react-test-renderer": "^16.4.2",
    "redux-devtools-extension": "^2.13.5",
    "rimraf": "^2.6.2",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.22.1",
    "stylelint": "^9.4.0",
    "stylelint-config-standard": "^18.2.0",
    "ts-jest": "^23.1.3",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.14.0",
    "tslint-plugin-prettier": "^1.3.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.0.1",
    "url-loader": "^1.0.1",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.7"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-actions": "^2.6.1"
  }
